
[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=apireal
Group=apireal
WorkingDirectory=/home/apireal/real_estate_accouting/RealEstate_Accouting/accounting
ExecStart=/home/apireal/real_estate_accouting/RealEstate_Accouting/accounting/venv/bin/gunicorn \
          --access-logfile - \
          --workers 2 \
          --bind 127.0.0.1:8089 \
          accounting.wsgi:application
Restart=always

[Install]
WantedBy=multi-user.target
